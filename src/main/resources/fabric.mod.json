  "authors": [
    "McJty",
    "AlphaMode"
  ],
  "contact": {
    "issues": "http://github.com/McJtyMods/TheOneProbe/issues",
    "sources": "http://github.com/McJtyMods/TheOneProbe/",
    "homepage": "http://github.com/McJtyMods/TheOneProbe/"
  },

  "license": "MIT",

  "environment": "*",
  "entrypoints": {
    "main": [
      "mcjty.theoneprobe.TheOneProbe"
    ],
    "client": [
      "mcjty.theoneprobe.rendering.ClientSetup"
    ]
  },
  "mixins": [
    "theoneprobe.mixin.json"
  ],
  "accessWidener": "theoneprobe.accesswidener",

  "depends": {
    "fabricloader": ">=0.12.12",
    "fabric": ">=0.46.0"
  }
}
